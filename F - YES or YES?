https://codeforces.com/group/Hv4K04mXo4/contest/662062/problem/F
#include <bits/stdc++.h>
using namespace std;

int main()
{
    ios::sync_with_stdio(false);
    cin.tie(nullptr);

    int t;
    cin >> t;

    while (t--)
    {
        string s;
        cin >> s;

        for (int i = 0; i < 3; i++)
            s[i] = tolower(s[i]);

        if (s == "yes")
            cout << "YES\n";
        else
            cout << "NO\n";
    }

    return 0;
}
